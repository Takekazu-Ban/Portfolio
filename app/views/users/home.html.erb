<div class="row">
  <div class="col-md-9 col-xs-12">
    <div class="home_list">
      <h5>新着サークル</h5>
      <%= render 'users/home_list',circles: @new %>
      <h5>人気サークル</h5>
      <%= render 'users/home_list',circles: @popularity %>
    </div>
  </div>

  <div class="col-md-3 col-xs-12">
    <div class="search">
      <%= render 'users/circles/search' %>
    </div>
    <div class="index_go">
      <%if @circle.ids == 0 %>
        <%= link_to users_circle_index_path(@circle.ids), method: :get do %>
          <h4>サークル一覧へ
          <span class="glyphicon glyphicon-arrow-right" aria-hidden="true"></span>
          </h4>
        <% end %>
      <% else %>
          <%= link_to users_circle_index_path(1), method: :get do %>
            <h4>サークル一覧へ
            <span class="glyphicon glyphicon-arrow-right" aria-hidden="true"></span>
            </h4>
          <% end %>
      <% end %>
    </div>
  </div>
</div>
