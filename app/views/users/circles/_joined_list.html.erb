<h3>参加サークル</h3>
  <table class="table">
    <% @circles_joined.each do |circle| %>
    <tr>
      <td><strong>
            <%= circle.circle_name %>
      </strong></td>
      <td>ジャンル：<%= circle.genre %></td>
      <td>
        <% if (controller.controller_name == 'host_circles' && controller.action_name == 'joining_show') || (controller.controller_name == 'circles' && controller.action_name == 'join_room') %>
            <%= link_to users_circle_join_room_path(circle.id) do %>
              <%= attachment_image_tag circle, :circle_img, :fill, 80, 60, format: 'jpeg' %>
            <% end %>

          <% else %>
            <%= link_to users_circle_path(circle.id) do %>
              <%= attachment_image_tag circle, :circle_img, :fill, 80, 60, format: 'jpeg' %>
            <% end %>
          <% end %>
      </td>
      <td>地域：<%= circle.active_area %></td>
      <td>メンバー数：<%= circle.circle_joins.count %>人</td>

      <td>
        <% if admin_signed_in? %>
          <%= link_to admins_user_path(circle.user.id) do %>
            <%= attachment_image_tag circle.user, :profile_image, format: 'jpeg',size:"50x50" , class: "img-circle" %>
          <% end %>
        <% else %>
          <%= link_to user_path(circle.user.id) do %>
            <%= attachment_image_tag circle.user, :profile_image, format: 'jpeg',size:"50x50" , class: "img-circle" %>
          <% end %>
        <% end %>
     </td>
    </tr>
    <% end %>
  </table>
  <%= paginate @circles_joined %>