<div class="admin_list">
  <h4>メンバー 一覧</h4>

  <div class="panel panel-primary">
    <% @users.each do |user| %>
      <div class="panel-heading">
        <strong><%= user.nickname %></strong>
      </div>
      <div class="panel-body">
        <div class="user_img">
          <%= link_to admins_user_path(user.id) do %>
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg' , class: "img-circle",fallback: "no_image.jpeg",size:"60x60" %>
            <% end %>
        </div>
        <div class="user_report">
          メールアドレス：<%= user.email %><br>
          性別：<%= user.gender %><br>
          年齢：<%= user.age %>歳
        </div>
      </div>
    <% end %>
  </div>
  <%= paginate @users %>
</div>
