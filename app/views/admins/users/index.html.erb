<h2>メンバー 一覧</h2>
<table class="table">
  <% @users.each do |user| %>
      <tr>
        <td><strong><%= user.nickname %></strong></td>
        <td>
          <%= link_to admins_user_path(user.id) do %>
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg' , class: "img-circle",fallback: "no_image.jpeg",size:"60x60" %>
            <% end %>
          </td>
        <td>メールアドレス：<%= user.email %></td>
        <td>性別：<%= user.gender %></td>
        <td>年齢：<%= user.age %>歳</td>
      </tr>
  <% end %>
</table>
<%= paginate @users %>